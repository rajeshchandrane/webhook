
select c3_job.job_title,c3_job.skill_set,c3_company.company_name,c3_job_application.first_name,c3_job_application.experience,c3_job_application.location,c3_job_application.email_id,c3_job_application.phone_no,c3_job_application.date_applied,c3_job_application.current_status from c3_job_application,c3_job,c3_company where c3_job_application.job_id = c3_job.id and c3_company.id=c3_job_application.company_id and c3_job_application.date_applied between CAST((DATE_SUB(NOW(),INTERVAL 15 DAY)) AS DATE) and NOW() and c3_job.job_title Like '%Software Enginee%';

select c3_job.job_title,c3_job.skill_set,c3_company.company_name,c3_job_application.first_name,c3_job_application.experience,c3_job_application.location,c3_job_application.email_id,c3_job_application.phone_no,c3_job_application.date_applied,c3_job_application.current_status from c3_job_application,c3_job,c3_company where c3_job_application.job_id = c3_job.id and c3_company.id=c3_job_application.company_id and c3_job_application.date_applied between CAST((DATE_SUB(NOW(),INTERVAL 15 DAY)) AS DATE) and NOW() and c3_job_application.location Like '%Bangalore%';

select c3_job.job_title,c3_job.skill_set,c3_company.company_name,c3_job_application.first_name,c3_job_application.experience,c3_job_application.location,c3_job_application.email_id,c3_job_application.phone_no,c3_job_application.date_applied,c3_job_application.current_status from c3_job_application,c3_job,c3_company where c3_job_application.job_id = c3_job.id and c3_company.id=c3_job_application.company_id and c3_job_application.date_applied between CAST((DATE_SUB(NOW(),INTERVAL 15 DAY)) AS DATE) and NOW() and c3_job.skill_set Like '%Php%';


select c3_job.job_title,c3_job.skill_set,c3_company.company_name,c3_job_application.first_name,c3_job_application.experience,c3_job_application.location,c3_job_application.email_id,c3_job_application.phone_no,c3_job_application.date_applied,c3_job_application.current_status
from c3_job_application,c3_job,c3_company where c3_job_application.job_id = c3_job.id and c3_company.id=c3_job_application.company_id and c3_job_application.date_applied between CAST((DATE_SUB(NOW(),INTERVAL 15 DAY)) AS DATE) and NOW() and c3_job.skill_set Like '%java%' or c3_job.skill_set Like '%spring%' or c3_job.skill_set Like '%AWS%' or c3_job.skill_set Like '%postgreSql%' or c3_job.skill_set Like '%NoSQL%' and c3_job_application.experience >= 3 and c3_job_application.experience <= 7 and c3_job_application.location Like '%Gurgaon%' and c3_job.job_title Like '%Software Enginee%';

or c3_job.skill_set Like '%spring%' or c3_job.skill_set Like '%AWS%' or c3_job.skill_set Like '%postgreSql%' or c3_job.skill_set Like '%NoSQL%'
